CREATE CACHED TABLE "PUBLIC"."USER_TABLE"(

    "ID" BIGINT GENERATED ALWAYS AS IDENTITY(START WITH 1 RESTART WITH 185) NOT NULL,

    "USERNAME" BINARY LARGE OBJECT NOT NULL,

    "PASSWORD" CHARACTER VARYING(100) NOT NULL,

    "ROLE" CHARACTER VARYING(100) NOT NULL

);

CREATE CACHED TABLE "PUBLIC"."ROUTE"(

    "ID" BIGINT GENERATED ALWAYS AS IDENTITY(START WITH 1 RESTART WITH 165) NOT NULL,

    "NAME" CHARACTER VARYING(25) NOT NULL,

    "WAYPOINTS" BINARY LARGE OBJECT NOT NULL,

    "USER_ID" BIGINT

);

CREATE CACHED TABLE "PUBLIC"."RIDE"(

    "ID" BIGINT GENERATED ALWAYS AS IDENTITY(START WITH 1 RESTART WITH 289) NOT NULL,

    "NAME" CHARACTER VARYING(25) NOT NULL,

    "RIDE_POINTS" BINARY LARGE OBJECT NOT NULL,

    "DISTANCE_TRAVELED" DECIMAL(10, 2) NOT NULL,

    "SPEED" DECIMAL(10, 2) NOT NULL,

    "AVG_SPEED" DECIMAL(10, 2) NOT NULL,

    "MAX_SPEED" DECIMAL(10, 2) NOT NULL,

    "USER_ID" BIGINT

);
